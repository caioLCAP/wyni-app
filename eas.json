{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "apk": {
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true
    },
    "appbundle": {
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "appstore": {
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "appstore": {
      "ios": {
        "appleId": "lupacap@gmail.com"
      }
    }
  }
}
